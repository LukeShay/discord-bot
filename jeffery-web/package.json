{
  "name": "svelte-app",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "@comments scripts": {
    "dev": "develop with blazing fast rebuilds",
    "dev:features": "develop with features like SSR and serviceworker enabled",
    "build": "run build scripts below",
    "build:app": "build single page application (SPA)",
    "build:static": "Generate static pages",
    "serve": "serve content in 'dist' folder",
    "rollup": "run the rollup bundler",
    "nollup": "run the nollup no-bundler",
    "routify": "run routify"
  },
  "scripts": {
    "dev": "run-p routify nollup",
    "dev:ssr": "run-p routify rollup",
    "build": "run-s build:*",
    "build:app": "routify -b && rollup -c",
    "build:static": "spank",
    "serve": "spassr --ssr",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify",
    "format": "prettier --plugin prettier-plugin-svelte --ignore-path .gitignore --write ./src/**/*",
    "check": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.2.0",
    "@roxi/routify": "^2.12.4",
    "@tsconfig/svelte": "^1.0.10",
    "@types/node": "^14.14.28",
    "autoprefixer": "^10.2.4",
    "axios": "^0.21.1",
    "axios-cache-adapter": "^2.7.0",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "fs-extra": "^9.1.0",
    "nollup": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.5",
    "rollup": "^2.39.0",
    "rollup-plugin-hot": "^0.1.1",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-svelte-hot": "^0.13.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-workbox": "^6.1.1",
    "spank": "^1.5.2",
    "spassr": "^2.2.0",
    "svelte": "^3.32.3",
    "svelte-check": "^1.1.34",
    "svelte-preprocess": "^4.6.9",
    "tailwindcss": "^2.0.3",
    "tossr": "^1.3.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "routify": {
    "extensions": "svelte,html,svx,md"
  },
  "spassr": {},
  "spank": {
    "blacklist": [
      "/example/modal/basic/4"
    ]
  }
}
